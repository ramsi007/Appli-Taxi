@model IEnumerable<Contrat>

@{
    Layout = "~/Views/Shared/_AdminLTE.cshtml";
}

<div class="m-5">
    <div class="white-Background border p-2">
        <div class="row">
            <div class="col-md-6">
                <h3 class="text-info">Liste des Contrats</h3>
            </div>
        </div>
    </div>
</div>


@if (Model.Count() == 0)
{
    <div class="col-md-12">
        <h4 class="text-center text-danger pt-4"> La liste des contrats est vide ... </h4>
    </div>

}
else
{
    <div id="view-all">
        @await Html.PartialAsync("_ViewAll")
    </div>
}

@section Scripts{

}
