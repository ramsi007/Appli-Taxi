@model IEnumerable<TypeCat>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLTE.cshtml";
}

<div class="m-5">
    <div class="white-Background border p-2">
        <div class="row">
            <div class="col-md-6">
                <h3 class="text-info">Liste des Type produits</h3>
            </div>
            <div class="col-md-6 text-right">
                <a onclick="showInPopup('@Url.Action("Create","TypeCats", null, Context.Request.Scheme)',
                'Créer un nouveau type produit')"
                   class="btn btn-primary text-white" style="cursor:pointer">
                    <i class="fas fa-plus"></i>&nbsp; Ajouter
                </a>
            </div>
        </div>
    </div>
</div>


@if (Model.Count() == 0)
{
    <div class="col-md-12">
        <h4 class="text-center text-danger pt-4"> Aucun type produit dans cette liste ... </h4>
    </div>

}
else
{
    <div id="view-all">
        @await Html.PartialAsync("_ViewAll")
    </div>
}

@section scripts
{

}

