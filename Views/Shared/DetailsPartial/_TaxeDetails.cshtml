@model Tax

<br />
<div class="form-group">
    <h4 class="text-success text-center">Détails du type </h4>
</div>

<br />
<br />

<div class="white-Background">
    @Html.HiddenFor(m => m.Id)
    <div class="row form-group">
        <div class="col-2">
            <label asp-for="Name" class="col-form-label"></label>
        </div>
        <div class="col-10">
            <input asp-for="Name" class="form-control" readonly />
        </div>
    </div>

    <div class="row form-group">
        <div class="col-2">
            <label asp-for="Discount" class="col-form-label"></label>
        </div>
        <div class="col-10">
            <input asp-for="Discount" class="form-control" readonly />
        </div>
    </div>
    <br />
    <partial name="_DetailsAndBAckToListButtonsPartial" model="@Model.Id" />

</div>


@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}
